{
    "go.inferGopath": false,
    "[markdown]": {
        "editor.cursorSurroundingLines": 0,
        "editor.tabSize": 4,
        "editor.wrappingIndent": "same"
    },
    "grammarly.selectors": [
        {
            "language": "python",
            "scheme": "file",
            "pattern": "algorithm-ds-templates/BFS-DFS/search.py"
        }
    ],
    "files.associations": {
        "*.jsonl": "jsonc",
        "iosfwd": "cpp",
        "vector": "cpp",
        "algorithm": "cpp",
        "unordered_map": "cpp",
        "chrono": "cpp",
        "limits": "cpp"
    },
