{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Test init-db",
            "program": "${workspaceFolder}/init-db.js",
            "request": "launch",
            "skipFiles": ["<node_internals>/**"],
            "type": "pwa-node"
        },
        {
            "name": "Start",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/app.js",
            "stopOnEntry": false,
            "args": [],
            "cwd": "${workspaceRoot}",
            "preLaunchTask": null,
            "runtimeExecutable": null,
            "runtimeArgs": ["--nolazy", "--use_strict"],
            "env": {
                "NODE_ENV": "development"
            },
            "externalConsole": false,
            "sourceMaps": false,
            "outDir": null
        }
    ]
}
